{
  "rest-client.environmentVariables": {
    "$shared": {
      "version": "v1"
    },
    "dev": {
      "host": "localhost:8888"
    },
    "stg": {
      "host": "stg.hoge.jp"
    },
    "prod": {
      "host": "prod.hoge.jp"
    }
  },
  "todo-tree.general.tags": ["FIXME", "NOTE", "TODO", "CHECK"],
  "todo-tree.highlights.defaultHighlight": {
    "gutterIcon": true
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check-circle-fill",
      "foreground": "orange",
      "iconColour": "orange"
    },
    "FIXME": {
      "icon": "flame",
      "foreground": "yellow",
      "iconColour": "yellow"
    },
    "NOTE": {
      "icon": "paintbrush",
      "foreground": "#00bfff",
      "iconColour": "#00bfff"
    },
    "CHECK": {
      "icon": "plus-circle",
      "foreground": "green",
      "iconColour": "green"
    }
  }
}
